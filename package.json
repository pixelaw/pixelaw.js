{
  "name": "pixelaw.js",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "build-cores": "bash ./scripts/build-core-packages.sh",
    "build": "bash ./scripts/build-packages.sh",
    "build-examples": "bash ./scripts/build-examples.sh",
    "clean": "bash ./scripts/clean.sh",
    "prettier-check": "pnpx prettier --check packages examples",
    "prettier": "pnpx prettier --write packages examples",
    "lint": "pnpm lint",
    "release": "pnpm build && pnpm -F './packages/**' publish -r --force",
    "publish": "pnpm -F './packages/**' publish -r --force",
    "docs": "npx typedoc --out docs",
    "prepare_NOPE": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@typhonjs-typedoc/typedoc-theme-dmt": "^0.3.1",
    "husky": "^9.1.7",
    "lerna": "^8.1.9",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.9",
    "typedoc-plugin-coverage": "^3.4.1"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.11"
  },
  "pnpm": {
    "patchedDependencies": {
      "@dojoengine/torii-wasm@1.2.5": "patches/@dojoengine__torii-wasm@1.2.5.patch"
    }
  }
}
